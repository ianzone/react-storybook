{
  "name": "react-storybook-template",
  "version": "0.0.0",
  "description": "react storybook template",
  "type": "module",
  "files": ["dist"],
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/style.css",
  "homepage": "https://github.com/ianzone/react-storybook#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianzone/react-storybook.git"
  },
  "scripts": {
    "lint": "biome check --fix src --reporter summary",
    "build": "vite build",
    "dev": "storybook dev -p 6006",
    "dev:cc": "rm ./node_modules/.cache -r && nr dev",
    "build-storybook": "storybook build",
    "postinstall": "lefthook install",
    "commit": "git add -A && git-cz",
    "release": "semantic-release -d"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-links": "^8.4.6",
    "@storybook/addon-onboarding": "^8.4.6",
    "@storybook/blocks": "^8.4.6",
    "@storybook/builder-vite": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "biome-extends": "^1.0.1",
    "git-cz": "^4.9.0",
    "happy-dom": "^15.11.7",
    "lefthook": "^1.8.4",
    "semantic-release": "^24.2.0",
    "storybook": "^8.4.6",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=22"
  }
}
